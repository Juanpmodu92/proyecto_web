{{! src/views/personas/contact.hbs }}
{{#> layout}}
<div class="container mt-5">
  <h1>Contact Us</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
      </tr>
    </thead>
    <tbody>
      {{#each contacts}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.phone}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
{{/layout}}
